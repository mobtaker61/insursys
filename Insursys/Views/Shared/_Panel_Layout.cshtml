    <script>


        function fn_GoMenu(serviceURL) {
            $("#loader_ms").html("");
            $("#full_fade").fadeIn(0);

            $.ajax({
                type: "POST",
                url: serviceURL,
                success: successFunc,
                error: errorFunc
            });
            function successFunc(data, status) { $("#content_wrapper").html(data); $("#full_fade").fadeOut(); }
            function errorFunc(data) { $("#loader_ms").html("No Connection<br>" + data); }
            location.href = "#" + decodeURI(serviceURL);
        }


        function fn_RelodeMap() {

            var pageName = location.href.split('#');
            if (pageName.length > 1) {
                fn_GoMenu(pageName[1]);
            }

        }
        fn_RelodeMap();
    </script>

