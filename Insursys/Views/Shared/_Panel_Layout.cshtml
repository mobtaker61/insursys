                    langName: langName
                }
                , success: function (data, status) {
                    $("#CurFlag").attr("class", "flag-xs flag-" + langName);
                    _get_data(langName);
                    $("#full_fade").fadeOut();
                    $(".navbar-btn.btn-group.open").attr("class","navbar-btn btn-group")
                    


                }
            });
        }


    </script>

