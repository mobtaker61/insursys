    <script>

        //===========================================================================  دریافت اطلاعات
        // از روی فایل ایکس ام ال :
        function _get_data(_LangName) {

            var i = 0;

            //اجرای دستور دریافت محتوا :
            var xmlhttp = new XMLHttpRequest();
            xmlhttp.onreadystatechange = function ()
            { if (xmlhttp.readyState == 4 && xmlhttp.status == 200) comand_into_adsl_service(xmlhttp); };
            xmlhttp.open("GET", "/App_DataStatic/Language/" + _LangName + ".xml", false);

            xmlhttp.setRequestHeader("Cache-Control", "no-cache");
            xmlhttp.send(null);

            //کد دستوری دریافت اطلاعات :
            function comand_into_adsl_service(xml) {
                var xmlDoc = xml.responseXML;
                x = xmlDoc.getElementsByTagName("CD");
                var count_item = xmlDoc.getElementsByTagName("CD").length;
                var content = "";

                for (var i = 0; i < count_item; i++) {
                    var TAGNAME = x[i].getElementsByTagName("TAGNAME")[0].childNodes[0].nodeValue;
                    var VALUE = x[i].getElementsByTagName("VALUE")[0].childNodes[0].nodeValue;
                    $(".lang_" + TAGNAME).html(VALUE);



                }
            }

        }
        _get_data('@Request.Cookies["langName"].Value');


